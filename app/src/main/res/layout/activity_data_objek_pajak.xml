<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/content"
    tools:context=".data_op.DataObjekPajak">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            android:id="@+id/ll">

            <LinearLayout
                android:clipToPadding="false"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.cardview.widget.CardView
                    android:foreground="?android:attr/selectableItemBackground"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">


                        <TextView
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Pilih Filter : "
                            android:fontFamily="@font/proximanovalight"
                            android:textSize="16dp"/>


                        <Spinner
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/spFilter"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"></Spinner>

                        <View
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginBottom="5dp"
                            android:background="#A3A3A3"/>

                        <LinearLayout
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:id="@+id/Lnpwpd">

                            <TextView
                                android:visibility="gone"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="NPWPD"
                                android:fontFamily="@font/proximanovalight"
                                android:textSize="16dp"/>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="left"
                                android:layout_marginBottom="10dp">

                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et1"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et2"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et3"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et4"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et5"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et6"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et7"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et8"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et9"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et10"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:id="@+id/et11"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>
                                <EditText
                                    android:layout_gravity="center"
                                    android:gravity="center"
                                    android:id="@+id/et12"
                                    android:fontFamily="@font/proximanovalight"
                                    android:textSize="18dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_npwpd"
                                    android:hint="0"
                                    android:maxLength="1"
                                    android:inputType="number"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:visibility="gone"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:id="@+id/Lnamawp">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="55dp"
                                android:fontFamily="@font/proximanovalight"
                                android:textColorHint="#535353">

                                <EditText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="16sp"
                                    android:hint="Masukkan Nama WP"
                                    android:textColor="#000"
                                    android:id="@+id/etNamaWPC"
                                    android:fontFamily="@font/proximanovalight"/>
                            </com.google.android.material.textfield.TextInputLayout>

                            <ImageView
                                android:id="@+id/Ivnamawpc"
                                android:layout_gravity="center"
                                android:layout_width="50dp"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/ic_search"/>
                        </LinearLayout>

                        <LinearLayout
                            android:visibility="gone"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:id="@+id/Lnamausaha">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="55dp"
                                android:fontFamily="@font/proximanovalight"
                                android:textColorHint="#535353">

                                <EditText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="16sp"
                                    android:hint="Masukkan Tempat Usaha"
                                    android:textColor="#000"
                                    android:id="@+id/etNamaTempatUsahaC"
                                    android:fontFamily="@font/proximanovalight"/>
                            </com.google.android.material.textfield.TextInputLayout>

                            <ImageView
                                android:id="@+id/Ivnamausahac"
                                android:layout_gravity="center"
                                android:layout_width="50dp"
                                android:layout_height="wrap_content"
                                android:background="@mipmap/ic_search"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <Spinner
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/spNamaWP"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"></Spinner>

                            <Spinner
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/spNamaUsaha"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"></Spinner>

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:visibility="gone"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:id="@+id/cvDetail"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_margin="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/proximanovalight"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="NPWPD"
                                android:textColor="#000"
                                android:id="@+id/etNPWPD"
                                android:fontFamily="@font/proximanovalight"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/proximanovalight"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Nama WP"
                                android:textColor="#000"
                                android:id="@+id/etNamaWP"
                                android:fontFamily="@font/proximanovalight"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <TextView
                            android:visibility="gone"
                            android:id="@+id/tvNamaUsaha"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Nama Tempat Usaha"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/proximanovalight"
                            android:layout_marginTop="5dp"
                            android:textColor="#363636"
                            android:layout_marginBottom="5dp"
                            />

                        <Spinner
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/spNUsaha"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"></Spinner>

                        <View
                            android:visibility="gone"
                            android:id="@+id/vNamaUsaha"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_margin="5dp"
                            android:background="#A3A3A3"/>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/proximanovalight"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:id="@+id/etNamaTempatUsaha"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Nama Tempat Usaha"
                                android:textColor="#000"
                                android:fontFamily="@font/proximanovalight"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Alamat Usaha"
                                android:textColor="#000"
                                android:id="@+id/etAlamatUsaha"
                                android:fontFamily="@font/proximanovalight"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:id="@+id/etKec"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Kecamatan"
                                android:textColor="#000"
                                android:fontFamily="@font/proximanovalight"
                                android:inputType="textCapCharacters"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:id="@+id/etKel"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Kelurahan"
                                android:textColor="#000"
                                android:fontFamily="@font/proximanovalight"
                                android:inputType="textCapCharacters"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:id="@+id/etJenisPajak"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Jenis Pajak"
                                android:textColor="#000"
                                android:fontFamily="@font/proximanovalight"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:textColorHint="#535353">

                            <EditText
                                android:editable="false"
                                android:enabled="false"
                                android:id="@+id/etGol"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="16sp"
                                android:hint="Golongan"
                                android:textColor="#000"
                                android:fontFamily="@font/proximanovalight"/>
                        </com.google.android.material.textfield.TextInputLayout>

                        <LinearLayout
                            android:id="@+id/LReklame"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:textColorHint="#535353">

                                <EditText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="16sp"
                                    android:hint="Lokasi Pasang"
                                    android:textColor="#000"
                                    android:enabled="false"
                                    android:editable="false"
                                    android:id="@+id/etLokasiPasang"
                                    android:fontFamily="@font/proximanovalight"/>
                            </com.google.android.material.textfield.TextInputLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <com.google.android.material.textfield.TextInputLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="0.4"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="5dp"
                                    android:textColorHint="#535353">

                                    <EditText
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textSize="16sp"
                                        android:hint="Panjang (m)"
                                        android:textColor="#000"
                                        android:editable="false"
                                        android:enabled="false"
                                        android:inputType="number"
                                        android:id="@+id/etPanjang"
                                        android:fontFamily="@font/proximanovalight"/>
                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="0.4"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="5dp"
                                    android:textColorHint="#535353">

                                    <EditText
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textSize="16sp"
                                        android:hint="Lebar (m)"
                                        android:inputType="number"
                                        android:textColor="#000"
                                        android:editable="false"
                                        android:enabled="false"
                                        android:id="@+id/etLebar"
                                        android:fontFamily="@font/proximanovalight"/>
                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    android:layout_width="0dp"
                                    android:layout_weight="0.4"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="5dp"
                                    android:textColorHint="#535353">

                                    <EditText
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textSize="16sp"
                                        android:hint="Sisi"
                                        android:inputType="number"
                                        android:textColor="#000"
                                        android:id="@+id/etSisi"
                                        android:editable="false"
                                        android:enabled="false"
                                        android:fontFamily="@font/proximanovalight"/>
                                </com.google.android.material.textfield.TextInputLayout>


                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Rokok"
                                        android:layout_marginLeft="5dp"
                                        android:fontFamily="@font/proximanovalight"
                                        android:layout_marginTop="5dp"
                                        android:textColor="#363636"
                                        />

                                    <CheckBox
                                        android:clickable="false"
                                        android:id="@+id/cbRokok"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"/>

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:layout_margin="5dp"
                                        android:background="#A3A3A3"/>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Ketinggian"
                                        android:layout_marginLeft="5dp"
                                        android:fontFamily="@font/proximanovalight"
                                        android:layout_marginTop="5dp"
                                        android:textColor="#363636"
                                        />

                                    <CheckBox
                                        android:clickable="false"
                                        android:id="@+id/cbKetinggian"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"/>

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:layout_margin="5dp"
                                        android:background="#A3A3A3"/>

                                </LinearLayout>


                            </LinearLayout>

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="5dp"
                                android:textColorHint="#535353">

                                <EditText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="16sp"
                                    android:hint="Teks"
                                    android:textColor="#000"
